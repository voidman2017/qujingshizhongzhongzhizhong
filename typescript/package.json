{
  "name": "typescript-learning",
  "version": "1.0.0",
  "description": "TypeScript学习项目",
  "main": "dist/typescript-learning.cjs",
  "module": "dist/typescript-learning.esm-bundler.js",
  "browser": "dist/typescript-learning.esm-bundler.js",
  "unpkg": "dist/typescript-learning.global.js",
  "jsdelivr": "dist/typescript-learning.global.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "node": "./dist/typescript-learning.esm-bundler.js",
        "default": "./dist/typescript-learning.esm-bundler.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "node": "./dist/typescript-learning.cjs",
        "default": "./dist/typescript-learning.cjs"
      }
    },
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "dev": "rollup -c --watch",
    "build": "rollup -c",
    "build:prod": "NODE_ENV=production rollup -c",
    "test": "vitest",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "typescript",
    "learning",
    "tutorial"
  ],
  "author": "princed",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@types/node": "^20.19.17",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "picocolors": "^1.1.1",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.52.2",
    "rollup-plugin-esbuild": "^6.2.1",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "chalk": "^5.3.0"
  }
}
